
<h1>Listing parking_spaces</h1>
<table class="table  table-bordered zebra-striped">
  <tr>
    <th>Location address</th>
    <th>Location x</th>
    <th>Location y</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parking_spaces.each do |parking_space| %>
  <tr>
    <td><%= parking_space.location_address %></td>
    <td><%= parking_space.location_x %></td>
    <td><%= parking_space.location_y %></td>
    <td><%= parking_space.user.email %></td>
    <td><%= link_to image_tag("icon-eye-open", :border => 0), parking_space %></td>
    <td><%= link_to 'Edit', edit_parking_space_path(parking_space) %></td>
    <td><%= link_to 'Destroy', parking_space, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>
<br />

<%= link_to 'New Parking space', new_parking_space_path %><br />
<% if !current_user.nil? %>
       <%= link_to 'Sign Out', '/users/sign_out', method: :delete  %>
<% else %>
       <%= link_to 'Sign in', '/users/sign_in'  %>
<% end %>
