		<div class="span12">
			<div class="row">
				<div class="span6">
					<span align="center">Start Date</span> <br/>
					<div id="datetimepicker2" class="input-append">
    				<%=f.text_field :start_date, :as => :string , "data-format" => "MM/dd/yyyy PP"%>
    				<span class="add-on">
	      			<i data-time-icon="icon-time" data-date-icon="icon-calendar"></i>
	    			</span>
					</div>
				</div>
				<div class="span6">
