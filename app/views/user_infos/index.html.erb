<h1>Listing user_infos</h1>

<table>
  <tr>
    <th>Address</th>
    <th>Proof type</th>
    <th>Proof</th>
    <th>Is admin</th>
    <th>Is trusted</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_infos.each do |user_info| %>
  <tr>
    <td><%= user_info.address %></td>
    <td><%= user_info.proof_type_id %></td>
    <td><%= user_info.proof_id %></td>
    <td><%= user_info.is_admin %></td>
    <td><%= user_info.is_trusted %></td>
    <td><%= user_info.user %></td>
    <td><%= link_to 'Show', user_info %></td>
    <td><%= link_to 'Edit', edit_user_info_path(user_info) %></td>
    <td><%= link_to 'Destroy', user_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User info', new_user_info_path %>
